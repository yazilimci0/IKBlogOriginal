@* @model IEnumerable<EFLayer.Class.Kategories>
 *@ 


@if (ViewData["kategoriler"] as IEnumerable<EFLayer.Class.Kategories> != null)
{
    @foreach (var item in ViewData["kategoriler"] as IEnumerable<EFLayer.Class.Kategories>)
    {
        <a class="d-block " href="Gonderis/mainPageKategories/@item.kategoriId">@item.kategoryName</a>

    }
}